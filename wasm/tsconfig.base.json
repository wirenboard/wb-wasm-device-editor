{
  "compilerOptions": {
    "target": "es2022",
    "strict": false,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "jsx": "react-jsx",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node"
  }
}
